<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:gifbox="clr-namespace:CookPopularControl.Controls.Animation">

    <Style x:Key="DefaultGifBoxStyle" TargetType="{x:Type gifbox:GifBox}">
        <Setter Property="Stroke" Value="{DynamicResource PrimaryColorTheme}" />
        <Setter Property="StrokeThickness" Value="{StaticResource Double20Value}" />
    </Style>

</ResourceDictionary>