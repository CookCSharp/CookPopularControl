<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimaryThemeColor">#19F8F8</Color>
    <Color x:Key="AssistantThemeColor">#3154DF</Color>
    <SolidColorBrush x:Key="PrimaryThemeBrush" Color="{StaticResource PrimaryThemeColor}" />
    <SolidColorBrush x:Key="AssistantThemeBrush" Color="{StaticResource AssistantThemeColor}" />

    <Color x:Key="BorderBrushColor">#07B5B5</Color>
    <Color x:Key="ButtonBaseMouseOverColor">#08E7E7</Color>
    <Color x:Key="ButtonBasePressColor">#07B5B5</Color>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderBrushColor}" />
    <SolidColorBrush x:Key="ButtonBaseMouseOverBackground" Color="{StaticResource ButtonBaseMouseOverColor}" />
    <SolidColorBrush x:Key="ButtonBasePressBackground" Color="{StaticResource ButtonBasePressColor}" />

    <Color x:Key="DataGridRowSelectedColor">#19F8F8</Color>
    <Color x:Key="DataGridRowMouseOverColor">#08E7E7</Color>
    <Color x:Key="DataGridRowMouseOverSelected">#07B5B5</Color>
    <Color x:Key="DataGirdCellFocusedColor">#3154DF</Color>

</ResourceDictionary>